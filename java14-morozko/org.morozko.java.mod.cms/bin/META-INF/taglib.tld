<name>styleId</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>
<attribute>
<name>style</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>
<attribute>
<name>target</name>
<required>false</required>
<rtexprvalue>true</rtexprvalue>
</attribute>
</tag>

<!-- TAG : link  -->
<tag>
<name>url</name>
<tagclass>org.morozko.java.mod.cms.web.tld.CmsUrlTag</tagclass>
<bodycontent>JSP</bodycontent>
